<section class="form-auth text-center">
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <!-- <img class="mb-4" src="" alt="" width="72" height="57">  -->
    <h1 class="h3 mb-3 fw-normal">Log In</h1>

    <%= f.email_field :email, autofocus: true, class: "form-control", id: "floatingInput", placeholder: "name@example.com", autocomplete: "current-email" %>
    <%= f.password_field :password, autocomplete: "current-password", placeholder: "password", class: "form-control", id: "floatingPassword" %>

    <div class="checkbox mb-3">
      <label>
        <input type="checkbox" value="remember-me"> Remember me
      </label>
    </div>
    <button class="w-100 btn btn-lg btn-primary" type="submit">Sign in</button>
    <p class="mt-5 mb-3 text-muted">© 2017–2021</p>
  <% end %>

  <%= render "devise/shared/links" %>
</section>